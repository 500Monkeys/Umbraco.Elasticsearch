@using Umbraco.Elasticsearch.Sample.Features.Search
@inherits UmbracoViewPage<Umbraco.Elasticsearch.Sample.Features.Search.Models.SearchResultModel<Umbraco.Elasticsearch.Sample.Features.Search.Queries.ArticleSearchResult, Umbraco.Elasticsearch.Sample.Features.Search.Queries.ArticleSearchParameters, Umbraco.Elasticsearch.Sample.Features.Search.Queries.ArticleDocument>>

<div>
    <h2>Search Results</h2>
    <ul>
        @foreach (var item in Model.Results.Documents)
        {
            <li>
                <h4>@item.Title</h4>
                <span>@item.Summary</span>
            </li>
        }
    </ul>
</div>